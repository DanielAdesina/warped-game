            </ImageView>
            <Button fx:id="playGame" layoutX="112.0" layoutY="141.0" mnemonicParsing="false" onAction="#buttonClickHandler" prefHeight="42.0" prefWidth="141.0" text="play game" textFill="#00c3ff">
